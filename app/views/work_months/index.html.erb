<p id="notice"><%= notice %></p>

<h1>Listing Work Months</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_months.each do |work_month| %>
      <tr>
        <td><%= work_month.name %></td>
        <td><%= link_to 'Show', work_month %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

